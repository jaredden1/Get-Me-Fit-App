<%- include('../partials/header') %>

<h1>Add Set Info</h1>
<form action="/workouts/<%= workoutId %>" method="POST">
  <label for="type">Type: </label>
  <select name="type" required>
    <option value="" disabled selected>Select Type</option>
    <option value="Incline">Incline Bench Press</option>
    <option value="Flat">Flat Bench Press</option>
    <option value="Decline">Decline Bench Press</option>
    <option value="Dumbbell">Dumbbell Press</option>
    <option value="Cable">Cable Fly</option>
    <option value="Pushups">Pushups</option>
  </select>
  <br />
  <label for="weight">Weight: </label>
  <select name="weight" required>
    <option value="" disabled selected>Select Weight</option>
    <option value="5">5 lbs</option>
    <option value="10">10 lbs</option>
    <option value="15">15 lbs</option>
    <option value="20">20 lbs</option>
    <option value="25">25 lbs</option>
    <option value="30">30 lbs</option>
    <option value="35">35 lbs</option>
    <option value="40">40 lbs</option>
    <option value="45">45 lbs</option>
    <option value="50">50 lbs</option>
    <option value="55">55 lbs</option>
    <option value="60">60 lbs</option>
    <option value="65">65 lbs</option>
    <option value="70">70 lbs</option>
    <option value="75">75 lbs</option>
    <!-- Add other weight options here -->
  </select>
  <br />
  <label for="reps">Reps: </label>
  <select name="reps" required>
    <option value="" disabled selected>Select Reps</option>
    <option value="5">5</option>
    <option value="10">10</option>
    <option value="15">15</option>
    <option value="20">20</option>
    <option value="25">25</option>
    <option value="30">30</option>
    <!-- Add other reps options here -->
  </select>
  <br />
  <label for="notes">Set Notes: </label>
  <input type="text" name="notes"/>
  <br />
  <input type="submit" value="Add Set" />
  <input type="hidden" name="entryId" value="<%= workoutId %>">
</form>
PARENT ID = <%= workoutId %>

<%- include('../partials/footer') %>